<hibernate-mapping>
  <class name="gov.hhs.fha.nhinc.conformance.dao.ConformanceMessage" table="conformancerepository">
    <id name="id">
      <generator class="native"/>
    </id>
    <property column="messageID" name="messageID"/>
    <property column="relatesToID" name="relatesToID"/>
    <property column="action" name="action"/>
    <property column="message" name="message"/>
    <property column="confTime" name="confTime"/>
    <property column="direction" name="direction"/>    
  </class>
  
  <!--<query name="findConformanceByMessageId">
    <![CDATA[FROM conformancedb co WHERE co.messageID = :messageID]]>
  </query>-->
 </hibernate-mapping>
